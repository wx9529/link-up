id}>\n                        <div style={{ backgroundImage: 'url(' + car.url + ')' }} className='card'>\n                        </div>\n                        <h3> {car.model}({car.year}) </h3>\n                        <h3>${car.price}</h3>\n                        <button type=\"submit\" onClick={() => deleteACar(car.id)} >Delete</button>\n                    </div>)}\n            </div>\n        </>\n    )\n}\nexport default OnBoarding\n\n\n\n\n\n\n\n\n\n\n",[],"/Users/apple/Desktop/link_up/client/src/components/ChatContainer.js",["97","98","99","100"],[],"/Users/apple/Desktop/link_up/client/src/components/AuthModal.js",["101","102"],[],"/Users/apple/Desktop/link_up/client/src/components/Nav.js",[],[],"/Users/apple/Desktop/link_up/client/src/components/MatchesDisplay.js",["103","104","105","106"],[],"/Users/apple/Desktop/link_up/client/src/components/ChatHeader.js",["107"],[],"/Users/apple/Desktop/link_up/client/src/components/ChatDisplay.js",["108"],[],"/Users/apple/Desktop/link_up/client/src/components/Chat.js",[],[],"/Users/apple/Desktop/link_up/client/src/components/ChatInput.js",[],[],"/Users/apple/Desktop/link_up/client/src/reportWebVitals.js",[],[],{"ruleId":"109","severity":1,"message":"110","line":8,"column":19,"nodeType":"111","messageId":"112","endLine":8,"endColumn":28},{"ruleId":"109","severity":1,"message":"113","line":8,"column":30,"nodeType":"111","messageId":"112","endLine":8,"endColumn":42},{"ruleId":"109","severity":1,"message":"114","line":10,"column":9,"nodeType":"111","messageId":"112","endLine":10,"endColumn":18},{"ruleId":"109","severity":1,"message":"115","line":2,"column":10,"nodeType":"111","messageId":"112","endLine":2,"endColumn":19},{"ruleId":"109","severity":1,"message":"116","line":4,"column":10,"nodeType":"111","messageId":"112","endLine":4,"endColumn":20},{"ruleId":"109","severity":1,"message":"117","line":5,"column":8,"nodeType":"111","messageId":"112","endLine":5,"endColumn":13},{"ruleId":"109","severity":1,"message":"110","l