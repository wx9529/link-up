y.</p>\n            <form onSubmit={handleSubmit}>\n                <input\n                    type=\"email\"\n                    id=\"email\"\n                    name=\"email\"\n                    placeholder=\"email\"\n                    required={true}\n                    onChange={(e) => setEmail(e.target.value)}\n                />\n                <input\n                    type=\"password\"\n                    id=\"password\"\n                    name=\"password\"\n                    placeholder=\"password\"\n                    required={true}\n                    onChange={(e) => setPassword(e.target.value)}\n                />\n                {isSignUp && <input\n                    type=\"password\"\n                    id=\"password-check\"\n                    name=\"password-check\"\n                    placeholder=\"confirm password\"\n                    required={true}\n                    onChange={(e) => setConfirmPassword(e.target.value)}\n                />}\n                <input className=\"secondary-button\" type=\"submit\" />\n                <p>{error}</p>\n            </form>\n\n            <hr />\n            <h2>GET THE APP</h2>\n\n        </div>\n    )\n}\nexport default AuthModal\n","/Users/apple/Desktop/link_up/client/src/components/Nav.js",[],[],"/Users/apple/Desktop/link_up/client/src/components/MatchesDisplay.js",["102","103","104","105"],[],"/Users/apple/Desktop/link_up/client/src/components/ChatHeader.js",["106"],[],"/Users/apple/Desktop/link_up/client/src/components/ChatDisplay.js",["107"],[],"/Users/apple/Desktop/link_up/client/src/components/Chat.js",[],[],"/Users/apple/Desktop/link_up/client/src/components/ChatInput.js",[],[],{"ruleId":"108","severity":1,"message":"109","line":8,"column":19,"nodeType":"110","messageId":"111","endLine":8,"endColumn":28},{"ruleId":"108","severity":1,"message":"112","line":8,"column":30,"nodeType":"110","messageId":"111","endLine":8,"endColumn":42},{"ruleId":"108","severity":1,"messag